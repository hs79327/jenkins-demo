pipeline {
  agent any
  stages {
    stage('stage1') {
      steps{
      echo "Test for Jenkinsfile"
      }
    }
    stage('stage2') {
      steps {
        echo "The jenkins file is working!!!"
      }
    }
    stage('stage3') {
        steps {
          echo "Now let's build a real CICD pipeline"
        }
    }
  }
}
